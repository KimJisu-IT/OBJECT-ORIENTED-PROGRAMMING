import java.awt.*;

public class FlowTestTwo {
	public FlowTestTwo() {
		Frame f= new Frame("FlowTestTwo");
		f.setLayout(new FlowLayout());
		f.add(new Button("1"));
		f.add(new Button("2"));
		f.add(new Button("3"));
		f.add(new Button("4"));
		f.add(new Button("5"));
		f.add(new Button("6"));
		f.add(new Button("7")).setVisible(false);
		f.add(new Button("8"));
		f.add(new Button("9"));
		f.add(new Button("10"));
		f.add(new Button("11"));
		f.setSize(300,200);
		f.setVisible(true);
	}
}